<settings>
	<servers>
		<!-- these id should match the id of the repo server in pom.xml -->
		<server>
			<id>oiq-nexus-releases</id>
			<username>deployment</username>
			<password>01q_dev12</password>
		</server>
		<server>
			<id>oiq-nexus-snapshots</id>
			<username>deployment</username>
			<password>01q_dev12</password>
		</server>
		<server>
			<id>oiq-nexus-3rdparty</id>
			<username>deployment</username>
			<password>01q_dev12</password>
		</server>
		<server>
			<id>oiq-central-repo</id>
			<username>maven</username>
			<password>01qM2build</password>
		</server>
		<server>
			<id>bitbucket.org</id>
			<username>git</username>
			<privateKey>${user.home}/.ssh/id_dsa</privateKey>
			<filePermissions>664</filePermissions>
			<directoryPermissions>775</directoryPermissions>
			<configuration></configuration>
		</server>
	</servers>

	<mirrors>
		<mirror>
			<id>oiq-central-repo</id>
			<url>https://repo.outsideiq.com/nexus/content/groups/public</url>
			<mirrorOf>*</mirrorOf>
		</mirror>
	</mirrors>

	<profiles>
		<profile>
			<id>enable-fleet</id>
			<activation>
				<activeByDefault>false</activeByDefault>
				<file>
					<exists>${user.home}/.m2/enable.fleet</exists>
				</file>
			</activation>
			<properties>
				<fleet.enabled>fleet</fleet.enabled>
			</properties>
		</profile>
		<profile>
			<id>enable-insure</id>
			<activation>
				<activeByDefault>false</activeByDefault>
				<file>
					<exists>${user.home}/.m2/enable.insure</exists>
				</file>
			</activation>
			<properties>
				<insure.enabled>insure</insure.enabled>
			</properties>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>fleet</activeProfile>
		<activeProfile>insure</activeProfile>
	</activeProfiles>
</settings> 
